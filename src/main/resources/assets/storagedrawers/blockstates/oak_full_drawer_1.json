{
    "multipart": [
        {   "when": { "facing": "north" },
            "apply": { "model":  "storagedrawers:block/oak_full_drawer_1.json" }
        },
        {   "when": { "facing": "east" },
            "apply": { "model":  "storagedrawers:block/oak_full_drawer_1.json", "y": 90 }
        },
        {   "when": { "facing": "south" },
            "apply": { "model":  "storagedrawers:block/oak_full_drawer_1.json", "y": 180 }
        },
        {   "when": { "facing": "west" },
            "apply": { "model":  "storagedrawers:block/oak_full_drawer_1.json", "y": 270 }
        },
        {   "when": { "facing": "north", "item_locked": true },
            "apply": { "model":  "storagedrawers:block/full_drawer_lock.json" }
        },
        {   "when": { "facing": "east", "item_locked": true },
            "apply": { "model":  "storagedrawers:block/full_drawer_lock.json", "y": 90 }
        },
        {   "when": { "facing": "south", "item_locked": true },
            "apply": { "model":  "storagedrawers:block/full_drawer_lock.json", "y": 180 }
        },
        {   "when": { "facing": "west", "item_locked": true },
            "apply": { "model":  "storagedrawers:block/full_drawer_lock.json", "y": 270 }
        }
    ]
}